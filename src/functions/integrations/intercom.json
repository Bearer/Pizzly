{
  "name": "Intercom",
  "config": {
    "authorizationURL": "https://app.intercom.io/oauth",
    "tokenURL": "https://api.intercom.io/auth/eagle/token",
    "authType": "OAUTH2",
    "tokenParams": {},
    "authorizationParams": {},
    "config": { "callbackURL": "https://int.bearer.sh/v2/auth/callback", "response_type": "code", "scope": [] },
    "provider": "Intercom",
    "hint": "You will find your Intercom credentials here: https://app.intercom.com/a/apps/_/developer-hub"
  },
  "request": {
    "baseURL": "https://api.intercom.io/",
    "headers": {
      "Accept": "application/json",
      "Authorization": "Bearer ${auth.accessToken}",
      "User-Agent": "Bearer.sh"
    }
  }
}
